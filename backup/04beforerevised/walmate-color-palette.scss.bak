// based color palette:
// https://material.io/guidelines/style/color.html#color-color-palette

$b-00: #$base00;
$b-01: #$base01;
$b-02: #$base02;
$b-03: #$base03;
$b-04: #$base04;
$b-05: #$base05;
$b-06: #$base06;
$b-07: #$base07; // white
$b-08: #$base08; // red
$b-09: #$base09; // orange
$b-0A: #$base0A; // yellow
$b-0B: #$base0B; // green
$b-0C: #$base0C; // cyan
$b-0D: #$base0D; // blue
$b-0E: #$base0E; // pink/purple
$b-0F: #$base0F; // orange2

$primary_color:         $b-0D;
$alt_primary_color:     darken($b-0D, 10%);
$progress_color:        lighten($b-0D, 10%);
$sidebar_color:         darken($b-07, 10%);

$theme_color_top:    lighten($b-0D, 10%);
$theme_color_middle: lighten($b-0D, 15%);
$theme_color_bottom: lighten($b-0D, 20%);


// Red
$red_50:   lighten($b-08, 50%);
$red_100:  lighten($b-08, 40%);
$red_200:  lighten($b-08, 30%);
$red_300:  lighten($b-08, 20%);
$red_400:  lighten($b-08, 10%);
$red_500:  $b-08;
$red_600:  darken($b-08, 10%);
$red_700:  darken($b-08, 20%);
$red_800:  darken($b-08, 30%);
$red_900:  darken($b-08, 40%);
$red_A100: desaturate($b-08, 80%) ;
$red_A200: desaturate($b-08, 40%) ;
$red_A400: scale-color($b-08, $saturation: -40%);
$red_A700: scale-color($b-08, $saturation: -80%);

